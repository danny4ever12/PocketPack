<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/wallback"
    tools:context="com.example.appdoctor.AboutMe"
    tools:ignore="MergeRootFrame" >

    <TextView
        android:id="@+id/aboutText1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/hello_world"
        android:textSize="@dimen/text_size" />

    <ImageButton
        android:id="@+id/imageBtn1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/violetback"
        android:contentDescription="@string/coffee"
        android:src="@drawable/button_selector" />

</TableLayout>